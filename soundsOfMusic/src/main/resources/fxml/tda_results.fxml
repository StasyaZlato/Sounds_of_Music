<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml"
      xmlns="http://javafx.com/javafx"
      fx:controller="controllers.TdaResultsControllers"
      prefHeight="400.0" prefWidth="600.0" spacing="20">
    <Label wrapText="true" visible="false" fx:id="descrPersistenceDiagram">
        <padding>
            <Insets top="20" right="20" bottom="20" left="20"/>
        </padding>
        Waveplot (based on lybrosa load result)
    </Label>
    <ImageView fx:id="persistenceDiagramImage"
               fitWidth="500"
               pickOnBounds="true"
               preserveRatio="true"/>
</VBox>

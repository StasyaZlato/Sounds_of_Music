<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<?import javafx.scene.image.ImageView?>
<?import javafx.geometry.Insets?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="controllers.GeneralStatisticsController"
          prefHeight="400.0" prefWidth="600.0">
    <Label fx:id="description" text="Very clever text describing what is happenning on this tab"
           GridPane.rowIndex="0" GridPane.columnIndex="0">
        <padding>
            <Insets top="15" right="15" bottom="15" left="5"/>
        </padding>
    </Label>

    <TableView fx:id="frequencyTable"
               GridPane.rowIndex="1" GridPane.columnIndex="0" GridPane.columnSpan="2" prefWidth="400.0">
        <columns>
            <TableColumn text="Chord" fx:id="chord"/>
            <TableColumn text="Frequency" fx:id="frequency"/>
        </columns>
    </TableView>
    <ImageView fx:id="graph" GridPane.rowIndex="2" GridPane.columnIndex="0"/>

</GridPane>
